<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>littleJ</display-name>
  <servlet>
    <servlet-name>Controller</servlet-name>
    <servlet-class>littleJWeb.Controller</servlet-class>
    <init-param>
      <param-name>dbUser</param-name>
      <param-value>pi</param-value>
    </init-param>
    <init-param>
      <param-name>dbPassword</param-name>
      <param-value>raspberry</param-value>
    </init-param>
    <init-param>
      <param-name>dbURL</param-name>
      <param-value>jdbc:mysql://192.168.1.61/littlej</param-value>
    </init-param>
    <init-param>
      <param-name>pageRefresh</param-name>
      <param-value>3</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>Controller</servlet-name>
    <url-pattern>/Controller.do</url-pattern>
  </servlet-mapping>
</web-app>

